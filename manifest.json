{
  "manifest_version": 2,
  "name": "AttendancerXP",
  "version": "0.2",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "page_action": {
    "default_icon": "icon.png"
  }
}
